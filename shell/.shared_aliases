# general
alias v='nvim'
alias cl='clear'
alias apt='apt-fast'
alias cat='batcat'
alias open='xdg-open'

# ls
alias l='lsd -l'
alias la='l -A'
alias lt='lsd --tree'
alias lte='lt -l'
alias lta='lt -A'
alias ltae='lta -l'

# git
alias gps='git push'
alias gpl='git pull'
alias gb='git branch'
alias gs='git status'
alias gd='git diff'
alias ga='git add'
alias gc='git commit'
alias gco='git checkout'
alias gl='git log'

